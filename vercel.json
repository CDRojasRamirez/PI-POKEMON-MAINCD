{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1",
        "headers": {
          "Cache-Control": "s-maxage=0"
        }
      }
    ]
  }
  